<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee 
	http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>admin</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/sindicato/lista.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>prepareInsert</from-outcome>
			<to-view-id>/sindicato/insert.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/sindicato/insert.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>insert</from-outcome>
			<to-view-id>/sindicato/lista.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>insertError</from-outcome>
			<to-view-id>/sindicato/insert.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>	
	
	<navigation-rule>
		<from-view-id>/sindicato/lista.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>show</from-outcome>
			<to-view-id>/sindicato/show.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/sindicato/show.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>prepareUpdate</from-outcome>
			<to-view-id>/sindicato/edit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/sindicato/edit.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>update</from-outcome>
			<to-view-id>/sindicato/lista.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>updateError</from-outcome>
			<to-view-id>/sindicato/edit.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/sindicato/show.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>delete</from-outcome>
			<to-view-id>/sindicato/lista.xhtml</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>deleteError</from-outcome>
			<to-view-id>/sindicato/lista.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
	

</faces-config>
